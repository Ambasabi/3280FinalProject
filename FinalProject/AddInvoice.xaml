<Window x:Class="FinalProject.AddInvoice"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Zoidberg's Invoice Creator" Height="161" Width="230"
        Background="{StaticResource WindowBackGround}"
        Icon="{StaticResource icon}">
    <Grid>
        <DatePicker x:Name="dpkSelectADate" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="202"/>
        <Button x:Name="btSubmit" Content="Submit" HorizontalAlignment="Left" Margin="32,103,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.467,1.773" Click="btSubmit_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="112,103,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
